html
    head
        title= 'Table'
        link(rel='stylesheet', href='table.css')
    body
        div(class='container')
            - for (var i = 0; i < 300; i++)
                each val, index in [10, 6, 4, 2, 2, 2, 2, 2]
                    div(class='ex ex-' + index)
                        - var n = 0
                        while n < val
                            div(class= 'col-' + n++ + ' col')= index + 1
